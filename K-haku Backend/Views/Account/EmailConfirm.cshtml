@using K_haku.Core.Application.ViewModels.User
@model string

@section Styles{
    <link href="~/css/User.css" rel="stylesheet" />
}
<div class="row mt-5">
    <div class="col-6 offset-3">
        <div class="card shadow-sm">
            <div class="card-body" >
                <div class="alert alert-primary alert-dismissible fade show" role="alert">
                    <h2>@Model - <a class="btn btn-primary" asp-controller="Account" asp-action="Index">Logueate</a></h2>
                    <button class="btn btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                </div>
            </div>
        </div>
    </div>

</div>
